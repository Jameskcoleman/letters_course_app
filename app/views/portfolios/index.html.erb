<%= render 'shared/header' %>
<div class="container">
  <div class='jumbotron'>
    <div class="row">
      <h2>Portfolios</h2>
    </div>
    <ul>
    <% @users.each do |user| %>
      <% if user.user_name != "Admin" and user.user_name != "Guest" %>
        <li><h3><a href="/portfolios/<%= user.id %>"><%= user.user_name %></a><h3></li>
      <% end %>
    <% end %>
  </div>
</div>